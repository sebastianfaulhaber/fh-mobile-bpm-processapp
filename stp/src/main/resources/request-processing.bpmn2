      <bpmn2:script><![CDATA[String targetURL = "https://sfaulhab.us.demos.redhatmobile.com/api/v2/ag-push/rest/sender";
String applicationId = "7594288e-2abc-4b18-862d-d72250c80ad9";
String masterSecret = "30184560-084d-4f73-9141-adf27c2d7c86";
byte[] basicAuthData = Base64.encodeBase64(new String(applicationId + ":" + masterSecret).getBytes());
String pushMessage = "{\"message\": {\"alert\": \"Your request is processed with ID 'VG000" + kcontext.getProcessInstance().getId() + "'.\",\"sound\": \"default\"}}";

HttpURLConnection connection = null;  
  try {
    //Create connection
    URL url = new URL(targetURL);
    connection = (HttpURLConnection)url.openConnection();
    connection.setRequestMethod("POST");
    connection.setRequestProperty("Content-Type", "application/json");
    connection.setRequestProperty("Accept", "application/json");
    //connection.setRequestProperty("Authorization", "Basic " + basicAuthData);
    connection.setRequestProperty("Authorization", "Basic NzU5NDI4OGUtMmFiYy00YjE4LTg2MmQtZDcyMjUwYzgwYWQ5OjMwMTg0NTYwLTA4NGQtNGY3My05MTQxLWFkZjI3YzJkN2M4Ng==");
    
    connection.setUseCaches(false);
    connection.setDoOutput(true);

    //Send request
    DataOutputStream wr = new DataOutputStream(connection.getOutputStream());
    wr.writeBytes(pushMessage);
    wr.close();

    //Get Response  
    InputStream is = connection.getInputStream();
    BufferedReader rd = new BufferedReader(new InputStreamReader(is));
    StringBuilder response = new StringBuilder(); // or StringBuffer if not Java 5+ 
    String line;
    while((line = rd.readLine()) != null) {
      response.append(line);
      response.append('\r');
    }
    rd.close();
    System.out.println("PUSH response: " + response.toString());
  } catch (Exception e) {
    e.printStackTrace();
    System.out.println("PUSH ERROR");
  } finally {
    if(connection != null) {
      connection.disconnect(); 
    }
  }]]></bpmn2:script>
